INSERT INTO O_TFR
	VALUES ("a12e23f7-8afb-47fa-80c2-4756eaffd439",
	"ed19a1be-ef18-40a2-9d35-d8318986c02e",
	'render_pragmas',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'// render the pragma string
pragma_string = "";
select many pragma_lists related by self->ooapragma_list[R3802];
for each list in pragma_lists
	pragma_string = pragma_string + list.render();
end for;

// select the proper OOA instance to edit the description
select one population related by self->ooapopulation[R3801];
if ( not_empty population )

	// check to see if we have any pragmas
	if ( "" != pragma_string )

		// put the pragmas in the description of the right object
		if ( "domain" == self.type )
			domain = population.current_component;
			domain.Descrip = pragma_string;
		else
		  TRACE::log( flavor:"failure", id:79, message:"Unable to find current ''" + self.type + "''" );
		end if;

	end if;

	// pop from the stack
	select one parent_element related by self->ooaelement[R3805.''child of''];
	unrelate population from self across R3801;
	if ( not_empty parent_element )
		relate population to parent_element across R3801;
		unrelate self from parent_element across R3805.''child of'';
	end if;

	// TODO delete self and children

end if;',
	1,
	'',
	"03266923-721c-4844-976a-fd6752d4055a");
