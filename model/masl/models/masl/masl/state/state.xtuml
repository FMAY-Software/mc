INSERT INTO O_TFR
	VALUES ("b5fb225a-420f-41b5-b16e-930a5ff3ea82",
	"64886a99-6942-49c6-b7d7-2dc495c52db1",
	'populate',
	'',
	"1b2a92d7-e97c-4b62-9e83-f7969ef6a845",
	0,
	'parent_object = param.object;

select any activity related by parent_object->activity[R3747]->state[R3704]->activity[R3704] where ( selected.name == param.name );
select one state related by activity->state[R3704];
if ( empty activity )

  select any parent_domain from instances of domain where false;
  select any parent_terminator from instances of terminator where false;

  activity = activity::populate( name:param.name, parent_domain:parent_domain, parent_terminator:parent_terminator, parent_object:parent_object );

  create object instance state of state;
  relate state to activity across R3704;

end if;

state.type = param.type;

return activity;',
	1,
	'',
	"00000000-0000-0000-0000-000000000000");
