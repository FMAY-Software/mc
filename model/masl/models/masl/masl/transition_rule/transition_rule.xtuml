INSERT INTO O_TFR
	VALUES ("09fd518e-8a2c-45a6-9ebd-9ce70a21ffdd",
	"e09b9ee1-811f-43ec-a7a8-302014f299d9",
	'validate',
	'',
	"ba5eda7a-def5-0000-0000-000000000001",
	0,
	'valid = true;
select many transitions from instances of transition_rule;
for each transition in transitions
  select one state related by transition->state[R3730];
  if (empty state)
    TRACE::log( flavor:"failure", id:50, message:"Transition has no start state." );
    valid = false;
  end if;
  select one transition_result related by transition->transition_result[R3731];
  if (empty state)
    TRACE::log( flavor:"failure", id:51, message:"Transition has no end state." );
    valid = false;
  end if;
  select one event related by transition->event[R3732];
  if (empty state)
    TRACE::log( flavor:"failure", id:52, message:"Transition has no event." );
    valid = false;
  end if;
end for;

// Validate siblings and subordinates.
return ( valid and transition_result::validate() );',
	1,
	'',
	"89e244ac-5bd2-4f0c-81ca-b9117b79256b");
