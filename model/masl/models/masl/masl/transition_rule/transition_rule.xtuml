	'render',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'select one transition_event related by self->event[R3732];
event = transition_event.name;
end = "";
select one state related by self->transition_result[R3731]->state[R3729];
if (empty state)
  select one nonstate related by self->transition_result[R3731]->nonstate_target[R3729];
  end = nonstate.type;
else
  end = state.name;
end if;
T::include( file:"masl/t.transition_render.masl" );
select one previous related by self->transition_rule[R3739.''precedes''];
if (not_empty previous)
  T::b( s:"," );
  previous.render();
end if;
',
	1,
	'',
	"61cbacc5-6b2e-43ae-9d15-deb00472cb96");
INSERT INTO O_TFR
	VALUES ("61cbacc5-6b2e-43ae-9d15-deb00472cb96",
