	'populate',
	'',
	"cad8a7f6-aa14-4a83-bf5b-84ec4a5baff1",
	0,
	'state = param.state;
event = param.event;
previous = param.previous_rule;
create object instance transition of transition_rule;
if (empty previous)
  relate transition to state across R3730;
else
  relate transition to previous across R3739.''succeeds'';
end if;
relate transition to event across R3732;
result = transition_result::populate(name:param.result, transition:transition);
return transition;',
	1,
	'',
	"00000000-0000-0000-0000-000000000000");
